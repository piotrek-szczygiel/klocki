    "version": "2.0.0",
    "tasks": [
        {
            "label": "run release",
            "type": "shell",
            "command": "cargo run --release",
            "problemMatcher": "$rustc"
        },
        {
            "label": "run",
            "type": "shell",
            "command": "cargo run",
            "problemMatcher": "$rustc"
        }
    ]
}
